
.home{
    display: flex;
    flex-direction: column;
}
.home-content{
    flex: 1;
    display: flex;
    flex-direction: column;
    
}

.home-search{
    z-index: 10;
    position: fixed;
    width: 100%;
    height:136rpx;
    background-color:#386CE2 ;
    border-radius: 0px 0px 40rpx 40rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.home-search>.input{
    width: 694rpx;
    height: 80rpx;
    background: #FFFFFF;
    opacity: 1;
    border-radius: 48rpx;
    display: flex;
    align-items: center;
}
.home-search>.input>image{
    width: 34rpx;
    height: 34rpx;
    margin-left: 26rpx;
}
.txt{
    margin-left: 42rpx;
    font-size: 14px;
    font-family: Source Han Sans CN;
    font-weight: 400;
    color: #999999;
}

.info{
    margin-top: 160rpx;
    flex: 1;
    /* overflow: auto; */
}

.no,.yes{
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
   
}

.no>image{
    margin-top: 388rpx;
    width:372rpx ;
    height:268rpx;

}
.no-border{
    margin-top: 62rpx;
    width: 216rpx;
    height: 63rpx;
    text-align: center;
    border: 2rpx solid #386CE2;
    border-radius: 32rpx;
    font-size: 12px;
    font-family: PingFang SC;
    font-weight: 400;
    line-height: 63rpx;
    color: #386CE2;
}


.yes-item{
    position: relative;
    width: 694rpx;
    height: 184rpx;
    display: flex;
    align-items:center;
    justify-content: space-between;
    margin-bottom: 20rpx;
    border-radius: 12rpx;
   
}
.yes-item>image{
   width: 124rpx;
   height: 124rpx; 
   margin-left: 30rpx;
   border-radius: 50%;
}

.yes-center{
    flex:1;
    margin-left: 26rpx;

}


.sex>image{
    width: 24rpx;
    height: 24rpx;
    margin-right: 22rpx;
}




.end-top,.end-bottom{
   
    text-align: right;
}

.yes-end{
    position: relative;
}
.end-top{
    position: absolute;
    top: 0;
}
.end-top>image{
   
    width: 111rpx;
    height: 110rpx;
   
}


.end-bottom{
    width: 162rpx;
    height: 44rpx;
    background: linear-gradient(91deg, #318CFF 0%, #0E56FF 100%);
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 60rpx 0px 0px 60rpx;
    font-size: 20rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #FFFFFF;
}

.center-top{
    font-size: 32rpx;
    font-family: PingFang SC;
    font-weight: 500;
    /* color: #0E56FF; */
    margin-bottom: 8rpx;    
}
.center-center{
    display: flex;
    align-items: center;
    font-size: 24rpx;
    font-family: PingFang SC;
    font-weight: 400;
    color: #333333;
    margin-bottom: 12rpx;
}
.center-bottom{
    font-size: 28rpx;
    font-family: Source Han Sans CN;
    font-weight: 400;
    line-height: 20px;
    color: #333333;
}

.yes-end{
    position: relative;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: flex-end;
 
}

.end-center{
    margin-top: 30rpx;
    font-size: 24rpx;
    font-family: Source Han Sans CN;
    font-weight: 400;
    color: #333333;
    margin-right:20rpx;
    margin-bottom: 18rpx;
}

.home{
    background: #F4F4FC;
}
.yes-item{
    background-color: #FFFFFF;
}
page{
    min-width: 100% !important;
}
.home{
    min-width: 100%;
    overflow: scroll;
    min-height: 100%;
}