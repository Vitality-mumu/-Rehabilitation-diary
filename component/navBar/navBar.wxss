.nav{
  position: fixed;
  top: 0;
  left: 0;
  width: 750rpx;
  color: rgb(247, 247, 247);
  z-index: 1000;
}
.nav-content{
    display: flex;
    align-items: center;
}
.back{
  margin-left: 28rpx;

  height: 100%;
  width: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
}
.backimg{
  width: 30rpx;
  height: 30rpx;
  background-image: url('https://wanbei.site/assets/img/temperature/left.png');
  background-size: cover;
  background-repeat: repeat;
}

.title{
  margin: auto;
  font-weight: bold;
}
.nav-bgc{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
}
